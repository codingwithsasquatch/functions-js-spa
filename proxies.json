{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "Logo": {
            "matchCondition": {
                "route": "/logo"
            },
            "backendUri": "%FUNCTION_URL%/api/GetFunctionLogo"
        },
        "Root": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "%STORAGE_URL_AND_CONTAINER%/functions-rock-even-more.html"
        }
    }
}